{
  "name": "alignment-theme-vsc",
  "displayName": "Alignment theme",
  "description": "A dark theme for Visual Studio Code",
  "version": "0.0.1",
  "publisher": "natixco",
  "license": "MIT",
  "engines": {
    "vscode": "^1.84.0"
  },
  "scripts": {
    "build": "npx tsx src/main.ts"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "theme",
    "dark"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/natixco/alignment-theme-vsc"
  },
  "icon": "icon.png",
  "contributes": {
    "themes": [
      {
        "label": "Alignment",
        "uiTheme": "vs-dark",
        "path": "./themes/alignment.json"
      },
      {
        "label": "Alignment (italic)",
        "uiTheme": "vs-dark",
        "path": "./themes/alignment-italic.json"
      },
      {
        "label": "Alignment darker",
        "uiTheme": "vs-dark",
        "path": "./themes/alignment-darker.json"
      },
      {
        "label": "Alignment darker (italic)",
        "uiTheme": "vs-dark",
        "path": "./themes/alignment-darker-italic.json"
      }
    ]
  },
  "devDependencies": {
    "@types/node": "^20.8.10",
    "polished": "^4.2.2"
  }
}
